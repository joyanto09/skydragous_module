<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_company_acpayee_image" model="ir.ui.view">
            <field name="name">res.company.ac.payee.image</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='favicon']"  position="after">
                    <field name="acpayee_image" widget="image" class="float-left oe_avatar" groups="base.group_no_one"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>